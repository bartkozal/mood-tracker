{
  "expo": {
    "name": "Mood Tracker",
    "slug": "mood-tracker",
    "privacy": "unlisted",
    "description": "Track daily mood with emojis",
    "githubUrl": "https://github.com/brtjkzl/mood-tracker",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#2290ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.brtjkzl.moodtracker",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.brtjkzl.moodtracker"
    }
  }
}
